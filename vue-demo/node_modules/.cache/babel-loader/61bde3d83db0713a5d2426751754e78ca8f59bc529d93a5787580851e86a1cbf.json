{"ast":null,"code":"import { reactive, ref } from \"vue\";\nexport default {\n  name: 'Products',\n  components: {},\n  setup() {\n    const dialogVisible = ref(false);\n    const openFormDialog = () => {\n      dialogVisible.value = true;\n    };\n    const closeFormDialog = () => {\n      dialogVisible.value = false;\n    };\n    const form = reactive({\n      name: '',\n      description: '',\n      quantity: 1,\n      imageUrl: ''\n    });\n    return {\n      dialogVisible,\n      openFormDialog,\n      closeFormDialog,\n      form\n    };\n  },\n  props: {\n    name: {\n      type: String,\n      required: true\n    },\n    price: {\n      type: Number,\n      required: true\n    },\n    quantity: {\n      type: Number,\n      default: 1\n    },\n    description: {\n      type: String,\n      default: 'default description'\n    },\n    imageUrl: {\n      type: String,\n      required: true\n    }\n  },\n  data() {\n    return {};\n  }\n};","map":{"version":3,"names":["reactive","ref","name","components","setup","dialogVisible","openFormDialog","value","closeFormDialog","form","description","quantity","imageUrl","props","type","String","required","price","Number","default","data"],"sources":["C:\\Users\\DELL\\Desktop\\网络应用开发课程设计\\vue-demo\\src\\components\\market\\Products.vue"],"sourcesContent":["<template>\r\n  <el-card :body-style=\"{ padding: '0px' }\" style=\"width: 200px;height: 300px\">\r\n    <img\r\n        :src=imageUrl\r\n        class=\"image\"\r\n        @click=\"openFormDialog\"\r\n    />\r\n    <div style=\"padding: 14px\">\r\n      <span>{{ name }}:{{ price }}元</span>\r\n      <br>\r\n      <span class=\"description\">{{ description }}</span>\r\n    </div>\r\n  </el-card>\r\n\r\n  <el-dialog title=\"编辑商品信息\" model-value=\"dialogVisible\" @close=\"closeFormDialog\" class=\"dialog-container\">\r\n    <el-form :model=\"form\" class=\"form-container\">\r\n      <el-form-item label=\"商品名称\">\r\n        <el-input v-model=\"form.name\"/>\r\n      </el-form-item>\r\n      <el-form-item label=\"商品描述\">\r\n        <el-input v-model=\"form.description\"/>\r\n      </el-form-item>\r\n      <el-form-item label=\"商品价格\">\r\n        <el-input v-model=\"form.price\"/>\r\n      </el-form-item>\r\n      <el-form-item>\r\n        <el-button type=\"primary\" @click=\"onSubmit\">保存</el-button>\r\n        <el-button type=\"danger\" @click=\"onDelete\">删除</el-button>\r\n        <el-button type=\"default\" @click=\"closeFormDialog\">取消</el-button>\r\n      </el-form-item>\r\n    </el-form>\r\n  </el-dialog>\r\n</template>\r\n\r\n<script>\r\n\r\n\r\nimport {reactive, ref} from \"vue\";\r\n\r\nexport default {\r\n  name: 'Products',\r\n  components: {},\r\n  setup() {\r\n    const dialogVisible = ref(false)\r\n\r\n    const openFormDialog=()=>{dialogVisible.value=true}\r\n\r\n    const closeFormDialog=()=>{dialogVisible.value=false}\r\n\r\n    const form = reactive({\r\n      name: '',\r\n      description: '',\r\n      quantity: 1,\r\n      imageUrl: '',\r\n    })\r\n\r\n    return{dialogVisible,openFormDialog,closeFormDialog,form}\r\n  },\r\n\r\n  props: {\r\n    name: {\r\n      type: String,\r\n      required: true,\r\n    },\r\n    price: {\r\n      type: Number,\r\n      required: true\r\n    },\r\n    quantity: {\r\n      type: Number,\r\n      default: 1\r\n    },\r\n    description: {\r\n      type: String,\r\n      default: 'default description'\r\n    },\r\n    imageUrl: {\r\n      type: String,\r\n      required: true\r\n    }\r\n  },\r\n  data() {\r\n    return {}\r\n  }\r\n\r\n};\r\n</script>\r\n\r\n<style>\r\n\r\n.image {\r\n  width: 200px;\r\n  height: 200px;\r\n  display: block;\r\n  object-fit: cover;\r\n}\r\n\r\n.description {\r\n  width: 200px;\r\n  height: 80px;\r\n}\r\n</style>"],"mappings":"AAqCA,SAAQA,QAAQ,EAAEC,GAAG,QAAO,KAAK;AAEjC,eAAe;EACbC,IAAI,EAAE,UAAU;EAChBC,UAAU,EAAE,CAAC,CAAC;EACdC,KAAKA,CAAA,EAAG;IACN,MAAMC,aAAY,GAAIJ,GAAG,CAAC,KAAK;IAE/B,MAAMK,cAAc,GAACA,CAAA,KAAI;MAACD,aAAa,CAACE,KAAK,GAAC,IAAI;I
{"ast":null,"code":"// eslint-disable-next-line no-unused-vars\nimport { reactive, ref } from \"vue\";\n// eslint-disable-next-line no-unused-vars\nimport axios from \"axios\";\nexport default {\n  name: 'Products',\n  components: {},\n  setup(props) {\n    const dialogVisible = ref(false);\n    const openFormDialog = () => {\n      dialogVisible.value = true;\n    };\n    const closeFormDialog = () => {\n      dialogVisible.value = false;\n    };\n    const form = reactive({\n      id: props.id,\n      name: props.name,\n      description: props.description,\n      quantity: props.quantity,\n      imageUrl: props.imageUrl,\n      price: props.price\n    });\n    const onSubmit = () => {\n      // 构建请求参数，包括商品ID和要更新的字段\n      const payload = {\n        id: form.id,\n        name: form.name,\n        description: form.description,\n        quantity: form.quantity,\n        imageUrl: form.imageUrl,\n        price: form.price\n      };\n      // 发送请求到后端，更新商品字段\n      axios.put('/products', payload).then(response => {\n        console.log('数据更新:', response.data);\n        closeFormDialog();\n      }).catch(error => {\n        // 请求失败处理逻辑\n        console.error(\"更新商品信息时出错\", error);\n      });\n    };\n    const onDelete = () => {\n      const payload = {\n        id: props.id,\n        name: props.name,\n        description: props.description,\n        quantity: props.quantity,\n        imageUrl: props.imageUrl,\n        price: props.price\n      };\n      axios.post('/delete', payload).then(closeFormDialog);\n    };\n    return {\n      dialogVisible,\n      openFormDialog,\n      closeFormDialog,\n      form,\n      onSubmit,\n      onDelete\n    };\n  },\n  props: {\n    name: {\n      type: String,\n      required: true\n    },\n    price: {\n      type: Number,\n      required: true\n    },\n    quantity: {\n      type: Number,\n      default: 1\n    },\n    description: {\n      type: String,\n      default: 'default description'\n    },\n    imageUrl: {\n      type: String,\n      required: true\n    },\n    id: {\n      type: Number,\n      required: true\n    }\n  },\n  data() {\n    return {};\n  }\n};","map":{"version":3,"names":["reactive","ref","axios","name","components","setup","props","dialogVisible","openFormDialog","value","closeFormDialog","form","id","description","quantity","imageUrl","price","onSubmit","payload","put","then","response","console","log","data","catch","error","onDelete","post","type","String","required","Number","default"],"sources":["C:\\Users\\DELL\\Desktop\\网络应用开发课程设计\\vue-demo\\src\\components\\market\\Products.vue"],"sourcesContent":["<template>\r\n  <el-card :body-style=\"{ padding: '0px' }\" style=\"width: 200px;height: 300px;position: relative\">\r\n    <img\r\n        :src=imageUrl\r\n        class=\"image\"\r\n        @click=\"openFormDialog\"\r\n    />\r\n    <div style=\"padding: 14px\" >\r\n      <span>{{ name }}:{{ price }}元</span>\r\n      <br>\r\n      <span class=\"description\">{{ description }}</span>\r\n    </div>\r\n    <el-button style=\"position: absolute;bottom: 1px\">购买</el-button>\r\n  </el-card>\r\n\r\n  <el-dialog title=\"编辑商品信息\" :model-value=\"dialogVisible\" @close=\"closeFormDialog\" class=\"dialog-container\">\r\n    <el-form :model=\"form\" class=\"form-container\">\r\n      <el-form-item label=\"商品名称\">\r\n        <el-input v-model=\"form.name\"/>\r\n      </el-form-item>\r\n      <el-form-item label=\"商品描述\">\r\n        <el-input v-model=\"form.description\"/>\r\n      </el-form-item>\r\n      <el-form-item label=\"商品价格\">\r\n        <el-input v-model=\"form.price\"/>\r\n      </el-form-item>\r\n      <el-form-item label=\"商品数量\">\r\n        <el-input v-model=\"form.quantity\"/>\r\n      </el-form-item>\r\n      <el-form-item label=\"图片链接\">\r\n        <el-input v-model=\"form.imageUrl\"/>\r\n      </el-form-item>\r\n      <el-form-item>\r\n        <el-button type=\"primary\" @click=\"onSubmit\">保存</el-button>\r\n        <el-button type=\"danger\" @click=\"onDelete\">删除</el-button>\r\n        <el-button type=\"default\" @click=\"closeFormDialog\">取消</el-button>\r\n      </el-form-item>\r\n    </el-form>\r\n  </el-dialog>\r\n</template>\r\n\r\n<script>\r\n\r\n\r\n// eslint-disable-next-line no-unused-vars\r\nimport {reactive, ref} from \"vue\";\r\n// eslint-disable-next-line no-unused-vars\r\nimport axios from \"axios\";\r\n\r\nexport default {\r\n  name: 'Products',\r\n  components: {},\r\n  setup(props) {\r\n    const dialogVisible = ref(false)\r\n\r\n    const openFormDialog = () => {\r\n      dialogVisible.value = true\r\n    }\r\n\r\n    const closeFormDialog = () => {\r\n      dialogVisible.value = false\r\n    }\r\n\r\n    const form = reactive({\r\n      id:props.id,\r\n      name: props.name,\r\n      description: props.description,\r\n      quantity: props.quantity,\r\n      imageUrl: props.imageUrl,\r\n      price:props.price,\r\n    })\r\n\r\n    const onSubmit = () => {\r\n      // 构建请求参数，包括商品ID和要更新的字段\r\n      const payload = {\r\n        id: form.id,\r\n        name: form.name,\r\n        description: form.description,\r\n        quantity: form.quantity,\r\n        imageUrl: form.imageUrl,\r\n        price:form.price\r\n      }\r\n      // 发送请求到后端，更新商品字段\r\n      axios.put('/products', payload)\r\n          .then(response => {\r\n            console.log('数据更新:', response.data);\r\n            closeFormDialog();\r\n          })\r\n          .catch(error => {\r\n            // 请求失败处理逻辑\r\n            console.error(\"更新商品信息时出错\", error);\r\n          });\r\n\r\n    };\r\n\r\n    const onDelete = () => {\r\n      const payload = {\r\n        id: props.id,\r\n        name: props.name,\r\n        description: props.description,\r\n        quantity: props.quantity,\r\n        imageUrl: props.imageUrl,\r\n        price: props.price\r\n      };\r\n      axios.post('/delete',payload)\r\n          .then(closeFormDialog)\r\n    }\r\n\r\n\r\n    return {dialogVisible, openFormDialog, closeFormDialog, form, onSubmit,onDelete}\r\n  },\r\n\r\n  props: {\r\n    name: {\r\n      type: String,\r\n      required: true,\r\n    },\r\n    price: {\r\n      type: Number,\r\n      required: true\r\n    },\r\n    quantity: {\r\n      type: Number,\r\n      default: 1\r\n    },\r\n    description: {\r\n      type: String,\r\n      default: 'default description'\r\n    },\r\n    imageUrl: {\r\n      type: String,\r\n      required: true\r\n    },\r\n    id: {\r\n      type: Number,\r\n      required: true\r\n    }\r\n  },\r\n  data() {\r\n    return {}\r\n  }\r\n\r\n};\r\n</script>\r\n\r\n<style>\r\n\r\n.image {\r\n  width: 200px;\r\n  height: 200px;\r\n  display: block;\r\n  object-fit: cover;\r\n}\r\n\r\n.description {\r\n  width: 200px;\r\n  height: 80px;\r\n}\r\n</style>"],"mappings":"AA4CA;AACA,SAAQA,QAAQ,EAAEC,GAAG,QAAO,KAAK;AACjC;AACA,OAAOC,KAAI,MAAO,OAAO;AAEzB,eAAe;EACbC,IAAI,EAAE,UAAU;EAChBC,UAAU,EAAE,CAAC,CAAC;EACdC,KAAKA,CAACC,KAAK,EAAE;IACX,MAAMC,aAAY,GAAIN,GAAG,CAAC,KAAK;IAE/B,MAAMO,cAAa,GAAIA,CAAA,KAAM;MAC3BD,aAAa,CAACE,KAAI,GAAI,IAAG;IAC3B;IAEA,MAAMC,eAAc,GAAIA,CAAA,KAAM;MAC5BH,aAAa,CAACE,KAAI,GAAI,KAAI;IAC5B;IAEA,MAAME,IAAG,GAAIX,QAAQ,CAAC;MACpBY,EAAE,EAACN,KAAK,CAACM,EAAE;MACXT,IAAI,EAAEG,KAAK,CAACH,IAAI;MAChBU,WAAW,EAAEP,KAAK,CAACO,WAAW;MAC9BC,QAAQ,EAAER,KAAK,CAACQ,QAAQ;MACxBC,QAAQ,EAAET,KAAK,CAACS,QAAQ;MACxBC,KAAK,EAACV,KAAK,CAACU;IACd,CAAC;IAED,MAAMC,QAAO,GAAIA,CAAA,KAAM;MACrB;MACA,MAAMC,OAAM,GAAI;QACdN,EAAE,EAAED,IAAI,CAACC,EAAE;QACXT,IAAI,EAAEQ,IAAI,CAACR,IAAI;QACfU,WAAW,EAAEF,IAAI,CAACE,WAAW;QAC7BC,QAAQ,EAAEH,IAAI,CAACG,QAAQ;QACvBC,QAAQ,EAAEJ,IAAI,CAACI,QAAQ;QACvBC,KAAK,EAACL,IAAI,CAACK;MACb;MACA;MACAd,KAAK,CAACiB,GAAG,CAAC,WAAW,EAAED,OAAO,EACzBE,IAAI,CAACC,QAAO,IAAK;QAChBC,OAAO,CAACC,GAAG,CAAC,OAAO,EAAEF,QAAQ,CAACG,IAAI,CAAC;QACnCd,eAAe,CAAC,CAAC;MACnB,CAAC,EACAe,KAAK,CAACC,KAAI,IAAK;QACd;QACAJ,OAAO,CAACI,KAAK,CAAC,WAAW,EAAEA,KAAK,CAAC;MACnC,CAAC,CAAC;IAER,CAAC;IAED,MAAMC,QAAO,GAAIA,CAAA,KAAM;MACrB,MAAMT,OAAM,GAAI;QACdN,EAAE,EAAEN,KAAK,CAACM,EAAE;QACZT,IAAI,EAAEG,KAAK,CA
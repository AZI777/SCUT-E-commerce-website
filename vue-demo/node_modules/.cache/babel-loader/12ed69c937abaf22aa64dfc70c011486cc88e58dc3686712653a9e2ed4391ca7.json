{"ast":null,"code":"import { ref } from 'vue';\nimport EditForm from './EditForm.vue';\nexport default {\n  // eslint-disable-next-line vue/multi-word-component-names\n  name: 'Products',\n  components: {\n    EditForm\n  },\n  setup() {\n    const books = ref([]);\n    const currentPage = ref(1);\n    const pageSize = 17;\n    const loadBooks = () => {\n      this.$axios.get('/books').then(resp => {\n        if (resp && resp.status === 200) {\n          books.value = resp.data;\n        }\n      });\n    };\n    const handleCurrentChange = currentPage => {\n      currentPage.value = currentPage;\n      console.log(currentPage.value);\n    };\n    const searchResult = () => {\n      this.$axios.get('/search?keywords=' + this.$refs.searchBar.keywords, {}).then(resp => {\n        if (resp && resp.status === 200) {\n          books.value = resp.data;\n        }\n      });\n    };\n    const deleteBook = id => {\n      this.$confirm('此操作将永久删除该书籍, 是否继续?', '提示', {\n        confirmButtonText: '确定',\n        cancelButtonText: '取消',\n        type: 'warning'\n      }).then(() => {\n        this.$axios.post('/delete', {\n          id: id\n        }).then(resp => {\n          if (resp && resp.status === 200) {\n            loadBooks();\n          }\n        });\n      }).catch(() => {\n        this.$message({\n          type: 'info',\n          message: '已取消删除'\n        });\n      });\n    };\n    const editBook = item => {\n      this.$refs.edit.dialogFormVisible = true;\n      this.$refs.edit.form = {\n        id: item.id,\n        cover: item.cover,\n        title: item.title,\n        author: item.author,\n        date: item.date,\n        press: item.press,\n        abs: item.abs,\n        category: {\n          id: item.category.id.toString(),\n          name: item.category.name\n        }\n      };\n    };\n    return {\n      books,\n      currentPage,\n      pageSize,\n      loadBooks,\n      handleCurrentChange,\n      searchResult,\n      deleteBook,\n      editBook\n    };\n  },\n  mounted() {\n    this.loadBooks();\n  }\n};","map":{"version":3,"names":["ref","EditForm","name","components","setup","books","currentPage","pageSize","loadBooks","$axios","get","then","resp","status","value","data","handleCurrentChange","console","log","searchResult","$refs","searchBar","keywords","deleteBook","id","$confirm","confirmButtonText","cancelButtonText","type","post","catch","$message","message","editBook","item","edit","dialogFormVisible","form","cover","title","author","date","press","abs","category","toString","mounted"],"sources":["C:\\Users\\DELL\\Desktop\\网络应用开发课程设计\\vue-demo\\src\\components\\market\\Product.vue"],"sourcesContent":["<template>\r\n  <div>\r\n    <el-row style=\"height: 840px;\">\r\n      <search-bar @onSearch=\"searchResult\" ref=\"searchBar\"></search-bar>\r\n      <el-tooltip effect=\"dark\" placement=\"right\"\r\n                  v-for=\"item in books.slice((currentPage-1)*pageSize, currentPage*pageSize)\"\r\n                  :key=\"item.id\">\r\n        <template #content>\r\n          <p style=\"font-size: 14px;margin-bottom: 6px;\">{{item.title}}</p>\r\n          <p style=\"font-size: 13px;margin-bottom: 6px\">\r\n            <span>{{item.author}}</span> /\r\n            <span>{{item.date}}</span> /\r\n            <span>{{item.press}}</span>\r\n          </p>\r\n          <p style=\"width: 300px\" class=\"abstract\">{{item.abs}}</p>\r\n        </template>\r\n        <el-card style=\"width: 135px;margin-bottom: 20px;height: 233px;float: left;margin-right: 15px\" class=\"book\"\r\n                 :body-style=\"{ padding: '10px' }\" shadow=\"hover\">\r\n          <div class=\"cover\" @click=\"editBook(item)\">\r\n            <img :src=\"item.cover\" alt=\"封面\">\r\n          </div>\r\n          <div class=\"info\">\r\n            <div class=\"title\">\r\n              <a href=\"\">{{item.title}}</a>\r\n            </div>\r\n            <i class=\"el-icon-delete\" @click=\"deleteBook(item.id)\"></i>\r\n          </div>\r\n          <div class=\"author\">{{item.author}}</div>\r\n        </el-card>\r\n      </el-tooltip>\r\n      <edit-form @onSubmit=\"loadBooks\" ref=\"edit\"></edit-form>\r\n    </el-row>\r\n    <el-row>\r\n      <el-pagination\r\n          @current-change=\"handleCurrentChange\"\r\n          :current-page=\"currentPage\"\r\n          :page-size=\"pageSize\"\r\n          :total=\"books.length\">\r\n      </el-pagination>\r\n    </el-row>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport { ref } from 'vue'\r\nimport EditForm from './EditForm.vue'\r\n\r\nexport default {\r\n  // eslint-disable-next-line vue/multi-word-component-names\r\n  name: 'Products',\r\n  components: { EditForm },\r\n  setup() {\r\n    const books = ref([])\r\n    const currentPage = ref(1)\r\n    const pageSize = 17\r\n\r\n    const loadBooks = () => {\r\n      this.$axios.get('/books').then((resp) => {\r\n        if (resp && resp.status === 200) {\r\n          books.value = resp.data\r\n        }\r\n      })\r\n    }\r\n\r\n    const handleCurrentChange = (currentPage) => {\r\n      currentPage.value = currentPage\r\n      console.log(currentPage.value)\r\n    }\r\n\r\n    const searchResult = () => {\r\n      this.$axios\r\n          .get('/search?keywords=' + this.$refs.searchBar.keywords, {})\r\n          .then((resp) => {\r\n            if (resp && resp.status === 200) {\r\n              books.value = resp.data\r\n            }\r\n          })\r\n    }\r\n\r\n    const deleteBook = (id) => {\r\n      this.$confirm('此操作将永久删除该书籍, 是否继续?', '提示', {\r\n        confirmButtonText: '确定',\r\n        cancelButtonText: '取消',\r\n        type: 'warning',\r\n      })\r\n          .then(() => {\r\n            this.$axios.post('/delete', { id: id }).then((resp) => {\r\n              if (resp && resp.status === 200) {\r\n                loadBooks()\r\n              }\r\n            })\r\n          })\r\n          .catch(() => {\r\n            this.$message({\r\n              type: 'info',\r\n              message: '已取消删除',\r\n            })\r\n          })\r\n    }\r\n\r\n    const editBook = (item) => {\r\n      this.$refs.edit.dialogFormVisible = true\r\n      this.$refs.edit.form = {\r\n        id: item.id,\r\n        cover: item.cover,\r\n        title: item.title,\r\n        author: item.author,\r\n        date: item.date,\r\n        press: item.press,\r\n        abs: item.abs,\r\n        category: {\r\n          id: item.category.id.toString(),\r\n          name: item.category.name,\r\n        },\r\n      }\r\n    }\r\n\r\n    return {\r\n      books,\r\n      currentPage,\r\n      pageSize,\r\n      loadBooks,\r\n      handleCurrentChange,\r\n      searchResult,\r\n      deleteBook,\r\n      editBook,\r\n    }\r\n  },\r\n\r\n  mounted() {\r\n    this.loadBooks()\r\n  },\r\n}\r\n</script>\r\n<style scoped>\r\n\r\n.cover {\r\n  width: 115px;\r\n  height: 172px;\r\n  margin-bottom: 7px;\r\n  overflow: hidden;\r\n  cursor: pointer;\r\n}\r\n\r\nimg {\r\n  width: 115px;\r\n  height: 172px;\r\n  /*margin: 0 auto;*/\r\n}\r\n\r\n.title {\r\n  font-size: 14px;\r\n  text-align: left;\r\n}\r\n\r\n.author {\r\n  color: #333;\r\n  width: 102px;\r\n  font-size: 13px;\r\n  margin-bottom: 6px;\r\n  text-align: left;\r\n}\r\n\r\n.abstract {\r\n  display: block;\r\n  line-height: 17px;\r\n}\r\n\r\n.el-icon-delete {\r\n  cursor: pointer;\r\n  float: right;\r\n}\r\n\r\n.switch {\r\n  display: flex;\r\n  position: absolute;\r\n  left: 780px;\r\n  top: 25px;\r\n}\r\n\r\na {\r\n  text-decoration: none;\r\n}\r\n\r\na:link, a:visited, a:focus {\r\n  color: #3377aa;\r\n}\r\n\r\n</style>\r\n"],"mappings":"AA4CA,SAASA,GAAE,QAAS,KAAI;AACxB,OAAOC,QAAO,MAAO,gBAAe;AAEpC,eAAe;EACb;EACAC,IAAI,EAAE,UAAU;EAChBC,UAAU,EAAE;IAAEF;EAAS,CAAC;EACxBG,KAAKA,CAAA,EAAG;IACN,MAAMC,KAAI,GAAIL,GAAG,CAAC,EAAE;IACpB,MAAMM,WAAU,GAAIN,GAAG,CAAC,CAAC;IACzB,MAAMO,QAAO,GAAI,EAAC;IAElB,MAAMC,SAAQ,GAAIA,CAAA,KAAM;MACtB,IAAI,CAACC,MAAM,CAACC,GAAG,CAAC,QAAQ,CAAC,CAACC,IAAI,CAAEC,IAAI,IAAK;QACvC,IAAIA,IAAG,IA
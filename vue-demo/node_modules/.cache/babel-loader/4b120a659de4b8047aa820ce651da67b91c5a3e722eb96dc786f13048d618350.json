{"ast":null,"code":"import { reactive, ref } from \"vue\";\nimport axios from \"axios\";\nexport default {\n  name: 'ProductsFrontend',\n  components: {},\n  setup(props) {\n    const dialogVisible = ref(false);\n    const openFormDialog = () => {\n      dialogVisible.value = true;\n    };\n    const closeFormDialog = () => {\n      dialogVisible.value = false;\n    };\n    const form = reactive({\n      id: props.id,\n      quantity: 1\n    });\n    const onSubmit = () => {\n      // 构建请求参数，包括商品ID和要更新的字段\n      const payload = {\n        product_id: form.id,\n        name: form.name,\n        description: form.description,\n        quantity: form.quantity,\n        imageUrl: form.imageUrl,\n        price: form.price\n      };\n      // 发送请求到后端，更新商品字段\n      axios.put('/products', payload).then(response => {\n        console.log('数据更新:', response.data);\n        closeFormDialog();\n      }).catch(error => {\n        // 请求失败处理逻辑\n        console.error(\"更新商品信息时出错\", error);\n      });\n    };\n    const onDelete = () => {\n      const payload = {\n        id: props.id,\n        name: props.name,\n        description: props.description,\n        quantity: props.quantity,\n        imageUrl: props.imageUrl,\n        price: props.price\n      };\n      axios.post('/delete', payload).then(closeFormDialog);\n    };\n    return {\n      dialogVisible,\n      openFormDialog,\n      closeFormDialog,\n      form,\n      onSubmit,\n      onDelete\n    };\n  },\n  props: {\n    name: {\n      type: String,\n      required: true\n    },\n    price: {\n      type: Number,\n      required: true\n    },\n    quantity: {\n      type: Number,\n      default: 1\n    },\n    description: {\n      type: String,\n      default: 'default description'\n    },\n    imageUrl: {\n      type: String,\n      required: true\n    },\n    id: {\n      type: Number,\n      required: true\n    }\n  },\n  data() {\n    return {};\n  }\n};","map":{"version":3,"names":["reactive","ref","axios","name","components","setup","props","dialogVisible","openFormDialog","value","closeFormDialog","form","id","quantity","onSubmit","payload","product_id","description","imageUrl","price","put","then","response","console","log","data","catch","error","onDelete","post","type","String","required","Number","default"],"sources":["C:\\Users\\DELL\\Desktop\\网络应用开发课程设计\\vue-demo\\src\\components\\market\\ProductsFrontend.vue"],"sourcesContent":["<template>\r\n  <el-card :body-style=\"{ padding: '0px' }\" style=\"width: 200px;height: 300px;position: relative\">\r\n    <img\r\n        :src=imageUrl\r\n        class=\"image\"\r\n    />\r\n    <div style=\"padding: 14px\">\r\n      <span>{{ name }}:{{ price }}元  库存:{{ quantity }}</span>\r\n      <br>\r\n      <span class=\"description\">{{ description }}</span>\r\n    </div>\r\n    <el-button style=\"position: absolute;bottom: 5px;left:0;width: 200px;\" @click=\"openFormDialog\">加购</el-button>\r\n  </el-card>\r\n\r\n  <el-dialog title=\"购买信息\" :model-value=\"dialogVisible\" @close=\"closeFormDialog\" class=\"dialog-container\">\r\n    <el-form :model=\"form\" class=\"form-container\">\r\n      <el-form-item label=\"购买数量\">\r\n        <el-input v-model=\"form.quantity\"/>\r\n      </el-form-item>\r\n\r\n      <el-form-item>\r\n        <el-button type=\"primary\" @click=\"onSubmit\">完成</el-button>\r\n        <el-button type=\"default\" @click=\"closeFormDialog\">取消</el-button>\r\n      </el-form-item>\r\n    </el-form>\r\n  </el-dialog>\r\n</template>\r\n\r\n<script>\r\n\r\n\r\nimport {reactive, ref} from \"vue\";\r\nimport axios from \"axios\";\r\n\r\nexport default {\r\n  name: 'ProductsFrontend',\r\n  components: {},\r\n  setup(props) {\r\n    const dialogVisible = ref(false)\r\n\r\n    const openFormDialog = () => {\r\n      dialogVisible.value = true\r\n    }\r\n\r\n    const closeFormDialog = () => {\r\n      dialogVisible.value = false\r\n    }\r\n\r\n    const form = reactive({\r\n      id: props.id,\r\n      quantity: 1,\r\n\r\n    })\r\n\r\n    const onSubmit = () => {\r\n      // 构建请求参数，包括商品ID和要更新的字段\r\n      const payload = {\r\n        product_id: form.id,\r\n        name: form.name,\r\n        description: form.description,\r\n        quantity: form.quantity,\r\n        imageUrl: form.imageUrl,\r\n        price: form.price\r\n      }\r\n      // 发送请求到后端，更新商品字段\r\n      axios.put('/products', payload)\r\n          .then(response => {\r\n            console.log('数据更新:', response.data);\r\n            closeFormDialog();\r\n          })\r\n          .catch(error => {\r\n            // 请求失败处理逻辑\r\n            console.error(\"更新商品信息时出错\", error);\r\n          });\r\n\r\n    };\r\n\r\n    const onDelete = () => {\r\n      const payload = {\r\n        id: props.id,\r\n        name: props.name,\r\n        description: props.description,\r\n        quantity: props.quantity,\r\n        imageUrl: props.imageUrl,\r\n        price: props.price\r\n      };\r\n      axios.post('/delete', payload)\r\n          .then(closeFormDialog)\r\n    }\r\n\r\n\r\n    return {dialogVisible, openFormDialog, closeFormDialog, form, onSubmit, onDelete}\r\n  },\r\n\r\n  props: {\r\n    name: {\r\n      type: String,\r\n      required: true,\r\n    },\r\n    price: {\r\n      type: Number,\r\n      required: true\r\n    },\r\n    quantity: {\r\n      type: Number,\r\n      default: 1\r\n    },\r\n    description: {\r\n      type: String,\r\n      default: 'default description'\r\n    },\r\n    imageUrl: {\r\n      type: String,\r\n      required: true\r\n    },\r\n    id: {\r\n      type: Number,\r\n      required: true\r\n    }\r\n  },\r\n  data() {\r\n    return {}\r\n  }\r\n\r\n};\r\n</script>\r\n\r\n<style>\r\n\r\n.image {\r\n  width: 200px;\r\n  height: 200px;\r\n  display: block;\r\n  object-fit: cover;\r\n}\r\n\r\n.description {\r\n  width: 200px;\r\n  height: 80px;\r\n}\r\n</style>"],"mappings":"AA+BA,SAAQA,QAAQ,EAAEC,GAAG,QAAO,KAAK;AACjC,OAAOC,KAAI,MAAO,OAAO;AAEzB,eAAe;EACbC,IAAI,EAAE,kBAAkB;EACxBC,UAAU,EAAE,CAAC
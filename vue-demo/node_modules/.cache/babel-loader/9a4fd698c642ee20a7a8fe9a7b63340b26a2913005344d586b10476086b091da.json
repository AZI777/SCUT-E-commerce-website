{"ast":null,"code":"import { ref, onMounted } from 'vue';\nimport EditForm from './EditForm.vue';\nimport axios from \"axios\";\nimport { ElMessageBox, ElMessage } from 'element-plus';\nexport default {\n  // eslint-disable-next-line vue/multi-word-component-names\n  name: 'Products',\n  components: {\n    EditForm\n  },\n  setup() {\n    const products = ref([]);\n    const currentPage = ref(1);\n    const pageSize = 17;\n    const editRef = ref(null);\n    const loadProducts = () => {\n      axios.get('/products').then(resp => {\n        if (resp && resp.status === 200) {\n          products.value = resp.data;\n        }\n      });\n    };\n    const handleCurrentChange = page => {\n      currentPage.value = page;\n      console.log(currentPage.value);\n    };\n    const deleteProduct = id => {\n      ElMessageBox.confirm('此操作将永久删除商品, 是否继续?', '提示', {\n        confirmButtonText: '确定',\n        cancelButtonText: '取消',\n        type: 'warning'\n      }).then(() => {\n        axios.post('/delete', {\n          id: id\n        }).then(resp => {\n          if (resp && resp.status === 200) {\n            loadProducts();\n          }\n        });\n      }).catch(() => {\n        ElMessage({\n          type: 'info',\n          message: '已取消删除'\n        });\n      });\n    };\n    const editProduct = item => {\n      editRef.value.dialogFormVisible = true;\n      editRef.value.form = {\n        id: item.id,\n        cover: item.cover,\n        title: item.title,\n        author: item.author,\n        date: item.date,\n        press: item.press,\n        abs: item.abs,\n        category: {\n          id: item.category.id.toString(),\n          name: item.category.name\n        }\n      };\n    };\n    onMounted(() => {\n      loadProducts();\n    });\n    return {\n      products,\n      currentPage,\n      pageSize,\n      editRef,\n      loadProducts,\n      handleCurrentChange,\n      deleteProduct,\n      editProduct\n    };\n  }\n};","map":{"version":3,"names":["ref","onMounted","EditForm","axios","ElMessageBox","ElMessage","name","components","setup","products","currentPage","pageSize","editRef","loadProducts","get","then","resp","status","value","data","handleCurrentChange","page","console","log","deleteProduct","id","confirm","confirmButtonText","cancelButtonText","type","post","catch","message","editProduct","item","dialogFormVisible","form","cover","title","author","date","press","abs","category","toString"],"sources":["C:\\Users\\DELL\\Desktop\\网络应用开发课程设计\\vue-demo\\src\\components\\market\\Product.vue"],"sourcesContent":["<template>\r\n  <div>\r\n    <el-row style=\"height: 840px;\">\r\n      <el-tooltip effect=\"dark\" placement=\"right\"\r\n                  v-for=\"item in products.slice((currentPage-1)*pageSize, currentPage*pageSize)\"\r\n                  :key=\"item.id\">\r\n        <template #content>\r\n          <p style=\"font-size: 14px;margin-bottom: 6px;\">{{item.title}}</p>\r\n          <p style=\"font-size: 13px;margin-bottom: 6px\">\r\n            <span>{{item.author}}</span> /\r\n            <span>{{item.date}}</span> /\r\n            <span>{{item.press}}</span>\r\n          </p>\r\n          <p style=\"width: 300px\" class=\"abstract\">{{item.abs}}</p>\r\n        </template>\r\n        <el-card style=\"width: 135px;margin-bottom: 20px;height: 233px;float: left;margin-right: 15px\" class=\"book\"\r\n                 :body-style=\"{ padding: '10px' }\" shadow=\"hover\">\r\n          <div class=\"cover\" @click=\"editProduct(item)\">\r\n            <img :src=\"item.cover\" alt=\"封面\">\r\n          </div>\r\n          <div class=\"info\">\r\n            <div class=\"title\">\r\n              <a href=\"\">{{item.title}}</a>\r\n            </div>\r\n            <i class=\"el-icon-delete\" @click=\"deleteProduct(item.id)\"></i>\r\n          </div>\r\n          <div class=\"author\">{{item.author}}</div>\r\n        </el-card>\r\n      </el-tooltip>\r\n      <edit-form @onSubmit=\"loadProducts\" ref=\"edit\"></edit-form>\r\n    </el-row>\r\n    <el-row>\r\n      <el-pagination\r\n          @current-change=\"handleCurrentChange\"\r\n          :current-page=\"currentPage\"\r\n          :page-size=\"pageSize\"\r\n          :total=\"products.length\">\r\n      </el-pagination>\r\n    </el-row>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport { ref, onMounted } from 'vue'\r\nimport EditForm from './EditForm.vue'\r\nimport axios from \"axios\";\r\nimport { ElMessageBox, ElMessage } from 'element-plus'\r\n\r\nexport default {\r\n  // eslint-disable-next-line vue/multi-word-component-names\r\n  name: 'Products',\r\n  components: { EditForm },\r\n  setup() {\r\n    const products = ref([])\r\n    const currentPage = ref(1)\r\n    const pageSize = 17\r\n    const editRef = ref(null)\r\n\r\n    const loadProducts = () => {\r\n      axios.get('/products').then((resp) => {\r\n        if (resp && resp.status === 200) {\r\n          products.value = resp.data\r\n        }\r\n      })\r\n    }\r\n\r\n    const handleCurrentChange = (page) => {\r\n      currentPage.value = page\r\n      console.log(currentPage.value)\r\n    }\r\n\r\n    const deleteProduct = (id) => {\r\n      ElMessageBox.confirm('此操作将永久删除商品, 是否继续?', '提示', {\r\n        confirmButtonText: '确定',\r\n        cancelButtonText: '取消',\r\n        type: 'warning',\r\n      })\r\n          .then(() => {\r\n            axios.post('/delete', { id: id }).then((resp) => {\r\n              if (resp && resp.status === 200) {\r\n                loadProducts()\r\n              }\r\n            })\r\n          })\r\n          .catch(() => {\r\n            ElMessage({\r\n              type: 'info',\r\n              message: '已取消删除',\r\n            })\r\n          })\r\n    }\r\n\r\n    const editProduct = (item) => {\r\n      editRef.value.dialogFormVisible = true\r\n      editRef.value.form = {\r\n        id: item.id,\r\n        cover: item.cover,\r\n        title: item.title,\r\n        author: item.author,\r\n        date: item.date,\r\n        press: item.press,\r\n        abs: item.abs,\r\n        category: {\r\n          id: item.category.id.toString(),\r\n          name: item.category.name,\r\n        },\r\n      }\r\n    }\r\n\r\n    onMounted(() => {\r\n      loadProducts()\r\n    })\r\n\r\n    return {\r\n      products,\r\n      currentPage,\r\n      pageSize,\r\n      editRef,\r\n      loadProducts,\r\n      handleCurrentChange,\r\n      deleteProduct,\r\n      editProduct,\r\n    }\r\n  },\r\n}\r\n</script>\r\n<style scoped>\r\n\r\n.cover {\r\n  width: 115px;\r\n  height: 172px;\r\n  margin-bottom: 7px;\r\n  overflow: hidden;\r\n  cursor: pointer;\r\n}\r\n\r\nimg {\r\n  width: 115px;\r\n  height: 172px;\r\n  /*margin: 0 auto;*/\r\n}\r\n\r\n.title {\r\n  font-size: 14px;\r\n  text-align: left;\r\n}\r\n\r\n.author {\r\n  color: #333;\r\n  width: 102px;\r\n  font-size: 13px;\r\n  margin-bottom: 6px;\r\n  text-align: left;\r\n}\r\n\r\n.abstract {\r\n  display: block;\r\n  line-height: 17px;\r\n}\r\n\r\n.el-icon-delete {\r\n  cursor: pointer;\r\n  float: right;\r\n}\r\n\r\n.switch {\r\n  display: flex;\r\n  position: absolute;\r\n  left: 780px;\r\n  top: 25px;\r\n}\r\n\r\na {\r\n  text-decoration: none;\r\n}\r\n\r\na:link, a:visited, a:focus {\r\n  color: #3377aa;\r\n}\r\n\r\n</style>\r\n"],"mappings":"AA2CA,SAASA,GAAG,EAAEC,SAAQ,QAAS,KAAI;AACnC,OAAOC,QAAO,MAAO,gBAAe;AACpC,OAAOC,KAAI,MAAO,OAAO;AACzB,SAASC,YAAY,EAAEC,SAAQ,QAAS,cAAa;AAErD,eAAe;EACb;EACAC,IAAI,EAAE,UAAU;EAChBC,UAAU,EAAE;IAAEL;EAAS,CAAC;EACxBM,KAAKA,CAAA,EAAG;IACN,MAAMC,QAAO,GAAIT,GAAG,CAAC,EAAE;IACvB,MAAMU,WAAU,GAAIV,GAAG,CAAC,CAAC;IACzB,MAAMW,QAAO,GAAI,EAAC;IAClB,MAAMC,OAAM,GAAIZ,GAAG,CAAC,IAAI;IAExB,MAAMa,YAAW,GAAIA,CAAA,KAAM;MACzBV,KAAK,CAACW,GAAG,CAAC,WAAW,CAAC,CAACC,IAAI,CAAEC,IAAI,IAAK;QACpC,IAAIA,IAAG,IAAKA,IAAI,CAACC,MAAK,KAAM,GAAG,EAAE;UAC/BR,QAAQ,CAACS,KAAI,GAAIF,IAAI,CAACG,IAAG;QAC3B;MACF,CAAC;IACH;IAEA,MAAMC,mBAAkB,GAAKC,IAAI,IAAK;MACpCX,WAAW,CAACQ,KAAI,GAAIG,IAAG;MACvBC,OAAO,CAACC,GAAG,CAACb,WAAW,CAACQ,KAAK;IAC/B;IAE
{"ast":null,"code":"import Products from '@/components/market/Products.vue';\nimport UploadForm from \"@/components/market/UploadForm.vue\";\nimport { onMounted, ref } from \"vue\";\nexport default {\n  name: 'ProductList',\n  components: {\n    Products,\n    UploadForm\n  },\n  setup() {\n    const products = ref([]);\n    const loadProduct = axios => {\n      axios.get('/products').then(response => {\n        if (response && response.status === 200) {\n          products.value = response.data;\n        }\n      });\n    };\n\n    // 在组合式 API 中使用 onMounted\n    onMounted(() => {\n      loadProduct();\n    });\n    return {\n      products,\n      loadProduct\n    };\n  }\n};","map":{"version":3,"names":["Products","UploadForm","onMounted","ref","name","components","setup","products","loadProduct","axios","get","then","response","status","value","data"],"sources":["C:\\Users\\DELL\\Desktop\\网络应用开发课程设计\\vue-demo\\src\\components\\market\\ProductList.vue"],"sourcesContent":["<template>\r\n  <div class=\"product-container\">\r\n    <div v-for=\"product in products\" :key=\"product.id\" class=\"product-item\">\r\n      <Products\r\n          :name=\"product.name\"\r\n          :price=\"product.price\"\r\n          :quantity=\"product.quantity\"\r\n          :description=\"product.description\"\r\n          :imageUrl=\"product.imageUrl\"\r\n      ></Products>\r\n    </div>\r\n    <div>\r\n      <UploadForm></UploadForm>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport Products from '@/components/market/Products.vue';\r\nimport UploadForm from \"@/components/market/UploadForm.vue\";\r\nimport {onMounted, ref} from \"vue\";\r\n\r\nexport default {\r\n  name: 'ProductList',\r\n  components: {\r\n    Products,\r\n    UploadForm,\r\n\r\n  },\r\n  setup() {\r\n    const products = ref([]);\r\n\r\n    const loadProduct = (axios) => {\r\n      axios\r\n          .get('/products')\r\n          .then(response => {\r\n            if (response && response.status =
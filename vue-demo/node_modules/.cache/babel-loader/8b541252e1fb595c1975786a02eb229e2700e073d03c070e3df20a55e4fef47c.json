{"ast":null,"code":"import { ref } from 'vue';\nimport axios from \"axios\";\nimport router from \"@/router\";\nexport default {\n  // eslint-disable-next-line vue/multi-word-component-names\n  name: 'Login',\n  setup() {\n    const loginForm = ref({\n      username: 'admin',\n      password: '123'\n    });\n    const login = async () => {\n      try {\n        const successResponse = await axios.post('/login', {\n          username: loginForm.value.username,\n          password: loginForm.value.password\n        });\n        if (successResponse.data.code === 200) {\n          router.replace({\n            path: '/index'\n          });\n        } else {\n          router.replace({\n            path: '/err'\n          });\n        }\n      } catch (error) {\n        // 处理错误\n      }\n    };\n    return {\n      loginForm,\n      login\n    };\n  }\n};","map":{"version":3,"names":["ref","axios","router","name","setup","loginForm","username","password","login","successResponse","post","value","data","code","replace","path","error"],"sources":["C:\\Users\\DELL\\Desktop\\网络应用开发课程设计\\vue-demo\\src\\components\\Login.vue"],"sourcesContent":["<template>\r\n  <body id=\"poster\">\r\n  <el-form class=\"login-container\" label-position=\"left\"\r\n           label-width=\"0px\">\r\n    <h3 class=\"login_title\">系统登录</h3>\r\n    <el-form-item>\r\n      <el-input type=\"text\" v-model=\"loginForm.username\"\r\n                auto-complete=\"off\" placeholder=\"账号\"></el-input>\r\n    </el-form-item>\r\n    <el-form-item>\r\n      <el-input type=\"password\" v-model=\"loginForm.password\"\r\n                auto-complete=\"off\" placeholder=\"密码\"></el-input>\r\n    </el-form-item>\r\n    <el-form-item style=\"width: 100%\">\r\n      <el-button type=\"primary\" style=\"width: 100%;background: #505458;border: none\" v-on:click=\"login\">登录</el-button>\r\n    </el-form-item>\r\n  </el-form>\r\n  </body>\r\n</template>\r\n\r\n<script>\r\nimport {ref} from 'vue';\r\nimport axios from \"axios\";\r\nimport router from \"@/router\";\r\n\r\nexport default {\r\n  // eslint-disable-next-line vue/multi-word-component-names\r\n  name: 'Login',\r\n  setup() {\r\n    const loginForm = ref({\r\n      username: 'admin',\r\n      password: '123'\r\n    });\r\n\r\n    const login = async () => {\r\n      try {\r\n        const successResponse = await axios.post('/login', {\r\n          username: loginForm.value.username,\r\n          password: loginForm.value.password\r\n        });\r\n\r\n        if (successResponse.data.code === 200) {\r\n          router.replace({path: '/index'});\r\n        } else {\r\n          router.replace({path: '/err'});\r\n        }\r\n      } catch (error) {\r\n        // 处理错误\r\n      }\r\n    };\r\n\r\n    return {\r\n      loginForm,\r\n      login\r\n    };\r\n  }\r\n};\r\n</script>\r\n\r\n<style>\r\n.login-container {\r\n  border-radius: 15px;\r\n  background-clip: padding-box;\r\n  margin: 90px auto;\r\n  width: 350px;\r\n  padding: 35px 35px 15px 35px;\r\n  background: #fff;\r\n  border: 1px solid #eaeaea;\r\n  box-shadow: 0 0 25px #cac6c6;\r\n}\r\n\r\n.login_title {\r\n  margin: 0px auto 40px auto;\r\n  text-align: center;\r\n  color: #505458;\r\n}\r\n\r\n#poster {\r\n  background: url(\"../assets/eva.jpg\") no-repeat;\r\n  background-position: center;\r\n  height: 100%;\r\n  width: 100%;\r\n  background-size: cover;\r\n  position: fixed;\r\n}\r\n\r\nbody {\r\n  margin: 0px;\r\n}\r\n\r\n\r\n</style>\r\n"],"mappings":"AAqBA,SAAQA,GAAG,QAAO,KAAK;AACvB,OAAOC,KAAI,MAAO,OAAO;AACzB,OAAOC,MAAK,MAAO,UAAU;AAE7B,eAAe;EACb;EACAC,IAAI,EAAE,OAAO;EACbC,KAAKA,CAAA,EAAG;IACN,MAAMC,SAAQ,GAAIL,GAAG,CAAC;MACpBM,QAAQ,EAAE,OAAO;MACjBC,QAAQ,EAAE;IACZ,CAAC,CAAC;IAEF,MAAMC,KAAI,GAAI,MAAAA,CAAA,KAAY;MACxB,IAAI;QACF,MAAMC,eAAc,GAAI,MAAMR,KAAK,CAACS,IAAI,CAAC,QAAQ,EAAE;UACjDJ,QAAQ,EAAED,SAAS,CAACM,KAAK,CAACL,QAAQ;UAClCC,QAAQ,EAAEF,SAAS,CAACM,KAAK,CAACJ;QAC5B,CAAC,CAAC;QAEF,IAAIE,eAAe,CAACG,IAAI,CAACC,IAAG,KAAM,GAAG,EAAE;UACrCX,MAAM,CAACY,OAAO,CAAC
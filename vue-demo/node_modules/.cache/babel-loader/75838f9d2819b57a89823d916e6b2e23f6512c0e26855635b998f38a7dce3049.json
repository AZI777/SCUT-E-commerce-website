{"ast":null,"code":"import { reactive, ref } from \"vue\";\nexport default {\n  name: 'Products',\n  components: {},\n  setup() {\n    const dialogVisible = ref(false);\n    const openFormDialog = () => {\n      dialogVisible.value = true;\n    };\n    const closeFormDialog = () => {\n      dialogVisible.value = false;\n    };\n    const form = reactive({\n      new_name: this.name,\n      new_description: this.description,\n      new_quantity: this.quantity,\n      new_imageUrl: this.imageUrl\n    });\n    const onSubmit = () => {\n      // 构建请求参数，包括商品ID和要更新的字段\n      const payload = {\n        pay_id: this.id,\n        pay_name: form.new_name,\n        pay_description: form.new_description,\n        pay_quantity: form.new_quantity,\n        pay_imageUrl: form.new_imageUrl\n      };\n\n      // 发送请求到后端，更新商品字段\n      axios.put(\"/api/products/\" + payload.id, payload).then(response => {\n        // 请求成功处理逻辑\n        console.log(\"商品信息已更新\");\n        closeFormDialog();\n      }).catch(error => {\n        // 请求失败处理逻辑\n        console.error(\"更新商品信息时出错\", error);\n      });\n    };\n    return {\n      dialogVisible,\n      openFormDialog,\n      closeFormDialog,\n      form,\n      onSubmit\n    };\n  },\n  props: {\n    name: {\n      type: String,\n      required: true\n    },\n    price: {\n      type: Number,\n      required: true\n    },\n    quantity: {\n      type: Number,\n      default: 1\n    },\n    description: {\n      type: String,\n      default: 'default description'\n    },\n    imageUrl: {\n      type: String,\n      required: true\n    },\n    id: {\n      type: Number,\n      required: true\n    }\n  },\n  data() {\n    return {};\n  }\n};","map":{"version":3,"names":["reactive","ref","name","components","setup","dialogVisible","openFormDialog","value","closeFormDialog","form","new_name","new_description","description","new_quantity","quantity","new_imageUrl","imageUrl","onSubmit","payload","pay_id","id","pay_name","pay_description","pay_quantity","pay_imageUrl","axios","put","then","response","console","log","catch","error","props","type","String","required","price","Number","default","data"],"sources":["C:\\Users\\DELL\\Desktop\\网络应用开发课程设计\\vue-demo\\src\\components\\market\\Products.vue"],"sourcesContent":["<template>\r\n  <el-card :body-style=\"{ padding: '0px' }\" style=\"width: 200px;height: 300px\">\r\n    <img\r\n        :src=imageUrl\r\n        class=\"image\"\r\n        @click=\"openFormDialog\"\r\n    />\r\n    <div style=\"padding: 14px\">\r\n      <span>{{ name }}:{{ price }}元</span>\r\n      <br>\r\n      <span class=\"description\">{{ description }}</span>\r\n    </div>\r\n  </el-card>\r\n\r\n  <el-dialog title=\"编辑商品信息\" :model-value=\"dialogVisible\" @close=\"closeFormDialog\" class=\"dialog-container\">\r\n    <el-form :model=\"form\" class=\"form-container\">\r\n      <el-form-item label=\"商品名称\">\r\n        <el-input v-model=\"form.name\"/>\r\n      </el-form-item>\r\n      <el-form-item label=\"商品描述\">\r\n        <el-input v-model=\"form.description\"/>\r\n      </el-form-item>\r\n      <el-form-item label=\"商品价格\">\r\n        <el-input v-model=\"form.price\"/>\r\n      </el-form-item>\r\n      <el-form-item>\r\n        <el-button type=\"primary\" @click=\"onSubmit\">保存</el-button>\r\n        <el-button type=\"danger\" @click=\"onDelete\">删除</el-button>\r\n        <el-button type=\"default\" @click=\"closeFormDialog\">取消</el-button>\r\n      </el-form-item>\r\n    </el-form>\r\n  </el-dialog>\r\n</template>\r\n\r\n<script>\r\n\r\n\r\nimport {reactive, ref} from \"vue\";\r\n\r\nexport default {\r\n  name: 'Products',\r\n  components: {},\r\n  setup() {\r\n    const dialogVisible = ref(false)\r\n\r\n    const openFormDialog=()=>{dialogVisible.value=true}\r\n\r\n    const closeFormDialog=()=>{dialogVisible.value=false}\r\n\r\n    const form = reactive({\r\n      new_name: this.name,\r\n      new_description: this.description,\r\n      new_quantity: this.quantity,\r\n      new_imageUrl: this.imageUrl,\r\n    })\r\n\r\n    const onSubmit = () => {\r\n      // 构建请求参数，包括商品ID和要更新的字段\r\n      const payload = {\r\n        pay_id:this.id,\r\n        pay_name:form.new_name,\r\n        pay_description:form.new_description,\r\n        pay_quantity:form.new_quantity,\r\n        pay_imageUrl:form.new_imageUrl\r\n\r\n\r\n    };\r\n\r\n      // 发送请求到后端，更新商品字段\r\n      axios.put(\"/api/products/\" + payload.id, payload)\r\n          .then(response => {\r\n            // 请求成功处理逻辑\r\n            console.log(\"商品信息已更新\");\r\n            closeFormDialog();\r\n          })\r\n          .catch(error => {\r\n            // 请求失败处理逻辑\r\n            console.error(\"更新商品信息时出错\", error);\r\n          });\r\n    };\r\n\r\n    return{dialogVisible,openFormDialog,closeFormDialog,form,onSubmit}\r\n  },\r\n\r\n  props: {\r\n    name: {\r\n      type: String,\r\n      required: true,\r\n    },\r\n    price: {\r\n      type: Number,\r\n      required: true\r\n    },\r\n    quantity: {\r\n      type: Number,\r\n      default: 1\r\n    },\r\n    description: {\r\n      type: String,\r\n      default: 'default description'\r\n    },\r\n    imageUrl: {\r\n      type: String,\r\n      required: true\r\n    },\r\n    id:{\r\n      type:Number,\r\n      required:true\r\n    }\r\n  },\r\n  data() {\r\n    return {}\r\n  }\r\n\r\n};\r\n</script>\r\n\r\n<style>\r\n\r\n.image {\r\n  width: 200px;\r\n  height: 200px;\r\n  display: block;\r\n  object-fit: cover;\r\n}\r\n\r\n.description {\r\n  width: 200px;\r\n  height: 80px;\r\n}\r\n</style>"],"mappings":"AAqCA,SAAQA,QAAQ,EAAEC,GAAG,QAAO,KAAK;AAEjC,eAAe;EACbC,IAAI,EAAE,UAAU;EAChBC,UAAU,EAAE,CAAC,CAAC;EACdC,KAAKA,CAAA,EAAG;IACN,MAAMC,aAAY,GAAIJ,GAAG,CAAC,KAAK;IAE/B,MAAMK,cAAc,GAACA,CAAA,KAAI;MAACD,aAAa,CAACE,KAAK,GAAC,IAAI;IAAA;IAElD,MAAMC,eAAe,GAACA,CAAA,KAAI;MAACH,aAAa,CAACE,KAAK,GAAC,KAAK;IAAA;IAEpD,MAAME,IAAG,GAAIT,QAAQ,CAAC;MACpBU,QAAQ,EAAE,IAAI,CAACR,IAAI;MACnBS,eAAe,EAAE,IAAI,CAACC,WAAW;MACjCC,YAAY,EAAE,IAAI,CAACC,QAAQ;MAC3BC,YAAY,EAAE,IAAI,
{"ast":null,"code":"import Products from '@/components/market/ProductsBackend.vue';\nimport UploadForm from \"@/components/market/UploadForm.vue\";\nimport { onMounted, ref, watch } from \"vue\";\nimport axios from \"axios\";\nexport default {\n  name: 'ProductListBackend',\n  components: {\n    Products,\n    UploadForm\n  },\n  setup() {\n    const products = ref([]);\n    const loadProduct = () => {\n      axios.get('/products').then(response => {\n        if (response && response.status === 200) {\n          products.value = response.data;\n        }\n      });\n    };\n\n    // 在组合式 API 中使用 onMounted\n    onMounted(() => {\n      loadProduct();\n    });\n    watch(products, () => loadProduct());\n    return {\n      products,\n      loadProduct\n    };\n  }\n};","map":{"version":3,"names":["Products","UploadForm","onMounted","ref","watch","axios","name","components","setup","products","loadProduct","get","then","response","status","value","data"],"sources":["C:\\Users\\DELL\\Desktop\\网络应用开发课程设计\\vue-demo\\src\\components\\market\\ProductListBackend.vue"],"sourcesContent":["<template>\r\n  <div class=\"product-container\">\r\n    <div v-for=\"product in products\" :key=\"product.id\" class=\"product-item\">\r\n      <Products\r\n          :name=\"product.name\"\r\n          :price=\"product.price\"\r\n          :quantity=\"product.quantity\"\r\n          :description=\"product.description\"\r\n          :imageUrl=\"product.imageUrl\"\r\n          :id=\"product.id\"\r\n      ></Products>\r\n    </div>\r\n    <div>\r\n      <UploadForm></UploadForm>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport Products from '@/components/market/ProductsBackend.vue';\r\nimport UploadForm from \"@/components/market/UploadForm.vue\";\r\nimport {onMounted, ref, watch} from \"vue\";\r\nimport axios from \"axios\";\r\n\r\nexport default {\r\n  name: 'ProductListBackend',\r\n  components: {\r\n    Products,\r\n    UploadForm,\r\n\r\n  },\r\n  setup() {\r\n    const 
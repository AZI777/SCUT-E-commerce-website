{"ast":null,"code":"import EditForm from './EditForm.vue';\nimport SearchBar from './SearchBar.vue';\nimport axios from 'axios';\nexport default {\n  name: 'Books',\n  components: {\n    EditForm,\n    SearchBar\n  },\n  data() {\n    return {\n      books: [],\n      currentPage: 1,\n      pageSize: 17\n    };\n  },\n  mounted() {\n    this.loadBooks();\n  },\n  computed: {\n    slicedBooks() {\n      const start = (this.currentPage - 1) * this.pageSize;\n      const end = this.currentPage * this.pageSize;\n      return this.books.slice(start, end);\n    }\n  },\n  methods: {\n    loadBooks() {\n      axios.get('/books').then(resp => {\n        if (resp && resp.status === 200) {\n          this.books = resp.data;\n        }\n      }).catch(error => {\n        console.error(error);\n      });\n    },\n    handleCurrentChange(currentPage) {\n      this.currentPage = currentPage;\n      console.log(this.currentPage);\n    },\n    searchResult() {\n      axios.get('/search?keywords=' + this.$refs.searchBar.keywords).then(resp => {\n        if (resp && resp.status === 200) {\n          this.books = resp.data;\n        }\n      }).catch(error => {\n        console.error(error);\n      });\n    },\n    deleteBook(id) {\n      this.$confirm('此操作将永久删除该书籍, 是否继续?', '提示', {\n        confirmButtonText: '确定',\n        cancelButtonText: '取消',\n        type: 'warning'\n      }).then(() => {\n        axios.post('/delete', {\n          id: id\n        }).then(resp => {\n          if (resp && resp.status === 200) {\n            this.loadBooks();\n          }\n        }).catch(error => {\n          console.error(error);\n        });\n      }).catch(() => {\n        this.$message({\n          type: 'info',\n          message: '已取消删除'\n        });\n      });\n    },\n    editBook(item) {\n      this.$refs.edit.dialogFormVisible = true;\n      this.$refs.edit.form = {\n        id: item.id,\n        cover: item.cover,\n        title: item.title,\n        author: item.author,\n        date: item.date,\n        press: item.press,\n        abs: item.abs,\n        category: {\n          id: item.category.id.toString(),\n          name: item.category.name\n        }\n      };\n    }\n  }\n};","map":{"version":3,"names":["EditForm","SearchBar","axios","name","components","data","books","currentPage","pageSize","mounted","loadBooks","computed","slicedBooks","start","end","slice","methods","get","then","resp","status","catch","error","console","handleCurrentChange","log","searchResult","$refs","searchBar","keywords","deleteBook","id","$confirm","confirmButtonText","cancelButtonText","type","post","$message","message","editBook","item","edit","dialogFormVisible","form","cover","title","author","date","press","abs","category","toString"],"sources":["C:\\Users\\DELL\\Desktop\\网络应用开发课程设计\\vue-demo\\src\\components\\market\\Books.vue"],"sourcesContent":["<template>\r\n  <div>\r\n    <el-row style=\"height: 840px;\">\r\n      <SearchBar @onSearch=\"searchResult\" ref=\"searchBar\"></SearchBar>\r\n      <el-tooltip effect=\"dark\" placement=\"right\"\r\n                  v-for=\"item in slicedBooks\"\r\n                  :key=\"item.id\">\r\n<!--        <p slot=\"content\" style=\"font-size: 14px;margin-bottom: 6px;\">{{item.title}}</p>-->\r\n<!--        <p slot=\"content\" style=\"font-size: 13px;margin-bottom: 6px\">-->\r\n<!--          <span>{{item.author}}</span> /-->\r\n<!--          <span>{{item.date}}</span> /-->\r\n<!--          <span>{{item.press}}</span>-->\r\n<!--        </p>-->\r\n<!--        <p slot=\"content\" style=\"width: 300px\" class=\"abstract\">{{item.abs}}</p>-->\r\n        <template #content>\r\n          <p style=\"font-size: 14px;margin-bottom: 6px;\">{{item.title}}</p>\r\n          <p style=\"font-size: 13px;margin-bottom: 6px\">\r\n            <span>{{item.author}}</span> /\r\n            <span>{{item.date}}</span> /\r\n            <span>{{item.press}}</span>\r\n          </p>\r\n          <p style=\"width: 300px\" class=\"abstract\">{{item.abs}}</p>\r\n        </template>\r\n        <el-card style=\"width: 135px;margin-bottom: 20px;height: 233px;float: left;margin-right: 15px\" class=\"book\"\r\n                 body-style=\"padding:10px\" shadow=\"hover\">\r\n          <div class=\"cover\" @click=\"editBook(item)\">\r\n            <img :src=\"item.cover\" alt=\"封面\">\r\n          </div>\r\n          <div class=\"info\">\r\n            <div class=\"title\">\r\n              <a href=\"\">{{item.title}}</a>\r\n            </div>\r\n            <i class=\"el-icon-delete\" @click=\"deleteBook(item.id)\"></i>\r\n          </div>\r\n          <div class=\"author\">{{item.author}}</div>\r\n        </el-card>\r\n      </el-tooltip>\r\n      <EditForm @onSubmit=\"loadBooks\" ref=\"edit\"></EditForm>\r\n    </el-row>\r\n    <el-row>\r\n      <el-pagination\r\n          @current-change=\"handleCurrentChange\"\r\n          :current-page=\"currentPage\"\r\n          :page-size=\"pageSize\"\r\n          :total=\"books.length\">\r\n      </el-pagination>\r\n    </el-row>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport EditForm from './EditForm.vue'\r\nimport SearchBar from './SearchBar.vue'\r\nimport axios from 'axios';\r\n\r\nexport default {\r\n  name: 'Books',\r\n  components: { EditForm, SearchBar },\r\n  data() {\r\n    return {\r\n      books: [],\r\n      currentPage: 1,\r\n      pageSize: 17\r\n    }\r\n  },\r\n  mounted() {\r\n    this.loadBooks()\r\n  },\r\n  computed: {\r\n    slicedBooks() {\r\n      const start = (this.currentPage - 1) * this.pageSize\r\n      const end = this.currentPage * this.pageSize\r\n      return this.books.slice(start, end)\r\n    }\r\n  },\r\n  methods: {\r\n    loadBooks() {\r\n      axios.get('/books')\r\n          .then(resp => {\r\n            if (resp && resp.status === 200) {\r\n              this.books = resp.data\r\n            }\r\n          })\r\n          .catch(error => {\r\n            console.error(error);\r\n          })\r\n    },\r\n    handleCurrentChange(currentPage) {\r\n      this.currentPage = currentPage\r\n      console.log(this.currentPage)\r\n    },\r\n    searchResult() {\r\n      axios.get('/search?keywords=' + this.$refs.searchBar.keywords)\r\n          .then(resp => {\r\n            if (resp && resp.status === 200) {\r\n              this.books = resp.data\r\n            }\r\n          })\r\n          .catch(error => {\r\n            console.error(error);\r\n          })\r\n    },\r\n    deleteBook(id) {\r\n      this.$confirm('此操作将永久删除该书籍, 是否继续?', '提示', {\r\n        confirmButtonText: '确定',\r\n        cancelButtonText: '取消',\r\n        type: 'warning'\r\n      })\r\n          .then(() => {\r\n            axios.post('/delete', { id: id })\r\n                .then(resp => {\r\n                  if (resp && resp.status === 200) {\r\n                    this.loadBooks()\r\n                  }\r\n                })\r\n                .catch(error => {\r\n                  console.error(error);\r\n                })\r\n          })\r\n          .catch(() => {\r\n            this.$message({\r\n              type: 'info',\r\n              message: '已取消删除'\r\n            })\r\n          })\r\n    },\r\n    editBook(item) {\r\n      this.$refs.edit.dialogFormVisible = true\r\n      this.$refs.edit.form = {\r\n        id: item.id,\r\n        cover: item.cover,\r\n        title: item.title,\r\n        author: item.author,\r\n        date: item.date,\r\n        press: item.press,\r\n        abs: item.abs,\r\n        category: {\r\n          id: item.category.id.toString(),\r\n          name: item.category.name\r\n        }\r\n      }\r\n    }\r\n  }\r\n}\r\n</script>\r\n<style scoped>\r\n\r\n.cover {\r\n  width: 115px;\r\n  height: 172px;\r\n  margin-bottom: 7px;\r\n  overflow: hidden;\r\n  cursor: pointer;\r\n}\r\n\r\nimg {\r\n  width: 115px;\r\n  height: 172px;\r\n  /*margin: 0 auto;*/\r\n}\r\n\r\n.title {\r\n  font-size: 14px;\r\n  text-align: left;\r\n}\r\n\r\n.author {\r\n  color: #333;\r\n  width: 102px;\r\n  font-size: 13px;\r\n  margin-bottom: 6px;\r\n  text-align: left;\r\n}\r\n\r\n.abstract {\r\n  display: block;\r\n  line-height: 17px;\r\n}\r\n\r\n.el-icon-delete {\r\n  cursor: pointer;\r\n  float: right;\r\n}\r\n\r\n.switch {\r\n  display: flex;\r\n  position: absolute;\r\n  left: 780px;\r\n  top: 25px;\r\n}\r\n\r\na {\r\n  text-decoration: none;\r\n}\r\n\r\na:link, a:visited, a:focus {\r\n  color: #3377aa;\r\n}\r\n\r\n</style>"],"mappings":"AAmDA,OAAOA,QAAO,MAAO,gBAAe;AACpC,OAAOC,SAAQ,MAAO,iBAAgB;AACtC,OAAOC,KAAI,MAAO,OAAO;AAEzB,eAAe;EACbC,IAAI,EAAE,OAAO;EACbC,UAAU,EAAE;IAAEJ,QAAQ;IAAEC;EAAU,CAAC;EACnCI,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,KAAK,EAAE,EAAE;MACTC,WAAW,EAAE,CAAC;MACdC,QAAQ,EAAE;IACZ;EACF,CAAC;EACDC,OAAOA,CAAA,EAAG;IACR,IAAI,CAACC,SAAS,CAAC;EACjB,CAAC;EACDC,QAAQ,EAAE;IACRC,WAAWA,CAAA,EAAG;MACZ,MAAMC,KAAI,GAAI,CAAC,IAAI,CAACN,WAAU,GAAI,CAAC,IAAI,IAAI,CAACC,QAAO;MACnD,MAAMM,GAAE,GAAI,IAAI,CAACP,WAAU,GAAI,IAAI,CAACC,QAAO;MAC3C,OAAO,IAAI,CAACF,KAAK,CAACS,KAAK,CAACF,KAAK,EAAEC,GAAG;IACpC;EACF,CAAC;EACDE,OAAO,EAAE;IACPN,SAASA,CAAA,EAAG;MACVR,KAAK,CAACe,GAAG,CAAC,QAAQ,EACbC,IAAI,CAACC,IAAG,IAAK;QACZ,IAAIA,IAAG,IAAKA,IAAI,CAACC,MAAK,KAAM,GAAG,EAAE;UAC/B,IAAI,CAACd,KAAI,GAAIa,IAAI,CAACd,IAAG;QACvB;MACF,CAAC,EACAgB,KAAK,CAACC,KAAI,IAAK;QACdC,OAAO,CAACD,KAAK,CAACA,KAAK,CAAC;MACtB,CAAC;IACP,CAAC;IACDE,mBAAmBA,CAACjB,WAAW,EAAE;MAC/B,IAAI,CAACA,WAAU,GAAIA,WAAU;MAC7BgB,OAAO,CAACE,GAAG,CAAC,IAAI,CAAClB,WAAW;IAC9B,CAAC;IACDmB,YAAYA,CAAA,EAAG;MACbxB,KAAK,CAACe,GAAG,CAAC,mBAAkB,GAAI,IAAI,CAACU,KAAK,CAACC,SAAS,CAACC,QAAQ,EACxDX,IAAI,CAACC,IAAG,IAAK;QACZ,IAAIA,IAAG,IAAKA,IAAI,CAACC,MAAK,KAAM,GAAG,EAAE;UAC/B,IAAI,CAACd,KAAI,GAAIa,IAAI,CAACd,IAAG;QACvB;MACF,CAAC,EACAgB,KAAK,CAACC,KAAI,IAAK;QACdC,OAAO,CAACD,KAAK,CAACA,KAAK,CAAC;MACtB,CAAC;IACP,CAAC;IACDQ,UAAUA,CAACC,EAAE,EAAE;MACb,IAAI,CAACC,QAAQ,CAAC,oBAAoB,EAAE,IAAI,EAAE;QACxCC,iBAAiB,EAAE,IAAI;QACvBC,gBAAgB,EAAE,IAAI;QACtBC,IAAI,EAAE;MACR,CAAC,EACIjB,IAAI,CAAC,MAAM;QACVhB,KAAK,CAACkC,IAAI,CAAC,SAAS,EAAE;UAAEL,EAAE,EAAEA;QAAG